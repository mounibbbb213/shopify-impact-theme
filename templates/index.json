    "custom_liquid_ay94aJ": {
      "type": "custom-liquid",
      "name": "Custom Liquid",
      "settings": {
        "full_width": true,
        "remove_vertical_spacing": false,
        "remove_horizontal_spacing": false,
        "liquid": "<!DOCTYPE html>\n<html lang=\"fr\">\n<head>\n  <meta charset=\"UTF-8\">\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n  <title>Évaluation du produit</title>\n  <style>\n    /* Styles de base */\n    .star-rating {\n      display: flex;\n      gap: 5px;\n      cursor: pointer;\n    }\n    .star-rating svg {\n      width: 40px;\n      height: 40px;\n      fill: #e0e0e0;\n      transition: fill 0.2s;\n    }\n    .star-rating svg.active,\n    .star-rating svg:hover,\n    .star-rating svg:hover ~ svg {\n      fill: #FFD700;\n    }\n    .rating-display {\n      margin-top: 10px;\n    }\n    .review-form {\n      margin-top: 20px;\n    }\n    .review-form input, .review-form textarea {\n      width: 100%;\n      padding: 10px;\n      margin: 10px 0;\n      border-radius: 5px;\n      border: 1px solid #ddd;\n    }\n    .review-form button {\n      padding: 10px 20px;\n      background-color: #FFD700;\n      border: none;\n      border-radius: 5px;\n      cursor: pointer;\n    }\n    .review-form button:hover {\n      background-color: #ffcc00;\n    }\n  </style>\n  <script src=\"https://code.jquery.com/jquery-3.6.0.min.js\"></script>\n</head>\n<body>\n  <div class=\"container\">\n    <h2>Évaluez ce produit</h2>\n\n    <!-- Étoiles de notation -->\n    <div class=\"star-rating\" data-rating=\"0\">\n      <svg data-value=\"1\" viewBox=\"0 0 20 20\"><polygon points=\"10,1 12.6,6.9 19,7.6 14,12.3 15.2,18.5 10,15.3 4.8,18.5 6,12.3 1,7.6 7.4,6.9\"/></svg>\n      <svg data-value=\"2\" viewBox=\"0 0 20 20\"><polygon points=\"10,1 12.6,6.9 19,7.6 14,12.3 15.2,18.5 10,15.3 4.8,18.5 6,12.3 1,7.6 7.4,6.9\"/></svg>\n      <svg data-value=\"3\" viewBox=\"0 0 20 20\"><polygon points=\"10,1 12.6,6.9 19,7.6 14,12.3 15.2,18.5 10,15.3 4.8,18.5 6,12.3 1,7.6 7.4,6.9\"/></svg>\n      <svg data-value=\"4\" viewBox=\"0 0 20 20\"><polygon points=\"10,1 12.6,6.9 19,7.6 14,12.3 15.2,18.5 10,15.3 4.8,18.5 6,12.3 1,7.6 7.4,6.9\"/></svg>\n      <svg data-value=\"5\" viewBox=\"0 0 20 20\"><polygon points=\"10,1 12.6,6.9 19,7.6 14,12.3 15.2,18.5 10,15.3 4.8,18.5 6,12.3 1,7.6 7.4,6.9\"/></svg>\n    </div>\n\n    <!-- Affichage de la note -->\n    <div class=\"rating-display\" id=\"rating-display\">0/5</div>\n\n    <!-- Formulaire pour le commentaire -->\n    <div class=\"review-form\">\n      <textarea id=\"comment\" rows=\"4\" placeholder=\"Laissez un commentaire (facultatif)\"></textarea>\n      <button id=\"submit-review\">Soumettre l'avis</button>\n    </div>\n  </div>\n\n  <script>\n    const stars = document.querySelectorAll('.star-rating svg');\n    const container = document.querySelector('.star-rating');\n    const ratingDisplay = document.getElementById('rating-display');\n    const submitButton = document.getElementById('submit-review');\n    let ratingValue = 0;\n\n    // Fonction pour survoler les étoiles et afficher la note en temps réel\n    stars.forEach((star) => {\n      star.addEventListener('mouseover', () => {\n        let val = parseInt(star.dataset.value);\n        highlightStars(val);\n      });\n\n      star.addEventListener('mouseout', () => {\n        resetStars();\n      });\n\n      star.addEventListener('click', () => {\n        ratingValue = parseInt(star.dataset.value);\n        container.setAttribute('data-rating', ratingValue);\n        highlightStars(ratingValue);\n        ratingDisplay.textContent = `${ratingValue}/5`;\n      });\n    });\n\n    // Met en surbrillance les étoiles\n    function highlightStars(rating) {\n      stars.forEach((s) => {\n        s.classList.remove('active');\n        if (parseInt(s.dataset.value) <= rating) {\n          s.classList.add('active');\n        }\n      });\n    }\n\n    // Réinitialise les étoiles à la note actuelle\n    function resetStars() {\n      highlightStars(ratingValue);\n    }\n\n    // Fonction pour envoyer l'avis via AJAX\n    submitButton.addEventListener('click', () => {\n      const comment = document.getElementById('comment').value;\n      if (ratingValue === 0) {\n        alert(\"Veuillez sélectionner une note.\");\n        return;\n      }\n\n      // Appel AJAX pour envoyer la note et le commentaire à Shopify\n      $.ajax({\n        url: '/apps/reviews/save',  // Remplacer par l'URL du point d'API de Shopify ou de ton application\n        type: 'POST',\n        data: {\n          rating: ratingValue,\n          comment: comment,\n          product_id: '1234567890'  // Remplace par l'ID du produit actuel\n        },\n        success: function(response) {\n          console.log('Avis enregistré', response);\n          alert('Merci pour votre avis !');\n          // Réinitialiser le formulaire\n          resetStars();\n          document.getElementById('comment').value = '';\n        },\n        error: function(xhr, status, error) {\n          console.error('Erreur lors de l\\'envoi de l\\'avis:', error);\n          alert('Une erreur est survenue lors de l\\'envoi de votre avis.');\n        }\n      });\n    });\n  </script>\n</body>\n</html>",
